# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.

/log/*
/tmp/*

db_dump

# Ignore asdf config file
.tool-versions

# Ignore debugger file
.byebug_history
notes.rb

# coverage files
coverage
coverage/*
test-rspec.xml

/.vagrant/*

# Ignore node_modules
node_modules/

# Ignore idea
/.idea

# Ignore generators
.generators

# Ignore binaries
.DS_Store

# Ignore pa11y screenshots
/pa11y/screenshots

# Ignore cucumber report results
features/cucumber-report/cucumber_report.html
features/cucumber-report/screenshot.png

# Ignore development env file
.env.development

# Ignore local storage
storage/*
/app/assets/builds/*
!/app/assets/builds/.keep

/node_modules
